<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	
	<TableLayout
		android:id="@+id/Month"
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content" 
		android:stretchColumns="7">
		<TableRow> 
			<TextView android:id="@+id/week1" 		
				android:layout_height="wrap_content" 
				android:layout_width="wrap_content" 
				android:text=".." />

			<TextView android:id="@+id/MonL" 		
				android:layout_height="wrap_content" 
				android:layout_width="wrap_content" 
				android:text="Mo" />
			<TextView android:id="@+id/Tw" 		
				android:layout_height="wrap_content" 
				android:layout_width="wrap_content" 
				android:text="W1" />
			<TextView android:id="@+id/week1" 		
				android:layout_height="wrap_content" 
				android:layout_width="wrap_content" 
				android:text="W1" />
			<TextView android:id="@+id/week1" 		
				android:layout_height="wrap_content" 
				android:layout_width="wrap_content" 
				android:text="W1" />
			<TextView android:id="@+id/week1" 		
				android:layout_height="wrap_content" 
				android:layout_width="wrap_content" 
				android:text="W1" />
			<TextView android:id="@+id/week1" 		
				android:layout_height="wrap_content" 
				android:layout_width="wrap_content" 
				android:text="W1" />
			<TextView android:id="@+id/week1" 		
				android:layout_height="wrap_content" 
				android:layout_width="wrap_content" 
				android:text="W1" />

			<ImageButton android:id="@+id/TimeSetUp"	
				android:padding="3dip"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"	
				android:src="@drawable/appointmentnew32x32"/>
			<ImageButton android:id="@+id/btnPeopleList" 	
				android:padding="3dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/people32x32"/>
			<TableLayout android:id="@+id/DateTim" 
				android:layout_width="fill_parent" 
				android:layout_height="wrap_content" 
				android:stretchColumns="0">
				<TableRow> 
					<TextView android:id="@+id/MeetingViewDateTitle" 
						android:layout_gravity="right" 
						android:layout_width="fill_parent" 
						android:layout_height="wrap_content" 
						android:textSize="16px" 
						android:text="Mon, 12/01/087" />
					<ImageView 
						android:layout_width="fill_parent" 
						android:layout_height="wrap_content" 
						android:src="@drawable/office_calendar16x16" />
				</TableRow>
				<TableRow> 
					<TextView 
						android:id="@+id/MeetingViewStartTitle"
						android:layout_gravity="right" 
						android:layout_width="fill_parent" 
						android:layout_height="wrap_content" 
						android:textSize="16px" 
						android:text=" 8:30"/>
					<ImageView 
						android:layout_width="fill_parent" 
						android:layout_height="wrap_content" 
						android:src="@drawable/appointementstart16x16" />
				</TableRow>
			</TableLayout>	
		</TableRow>
	</TableLayout>
</LinearLayout>