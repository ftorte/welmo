<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:textSize="22px">

	<TableLayout android:id="@+id/Calendar"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:stretchColumns="3"
		android:shrinkColumns="3" android:clickable="true">

		<TableRow>
			<ImageView android:id="@+id/PrevMonth"
				android:layout_height="10px" android:src="@drawable/go_previous"
				android:layout_width="wrap_content" />
			<TextView android:id="@+id/MontLabel"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content" android:text="Januarry" />
			<ImageView android:id="@+id/PrevMonth"
				android:layout_height="10px" android:src="@drawable/go_next"
				android:layout_width="wrap_content" />

			<TextView android:layout_height="wrap_content"
				android:layout_width="wrap_content" android:text="" />


			<TextView android:id="@+id/YearLabel"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content" android:text="2008" />
		</TableRow>

	</TableLayout>

	<com.welmo.calendar.CalendarMonth android:id="@+id/Month"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:stretchColumns="1,2,3,4,5,6,7"
		android:shrinkColumns="1,2,3,4,5,6,7" android:clickable="true">

		<TableRow android:id="@+id/MonthRow0">
			
			<TextView 
				
				android:layout_height="wrap_content"
				android:layout_width="wrap_content" android:text="  " />
				 
			<TextView android:id="@+id/MonLabel"
				android:layout_height="wrap_content"
				style="@style/clendarworkingdaylable"
				android:layout_width="wrap_content" android:text="Mo" />
			<TextView android:id="@+id/TweLbel"
				android:layout_height="wrap_content"
				style="@style/clendarworkingdaylable"
				android:layout_width="wrap_content" android:text="Tu" />
			<TextView android:id="@+id/WenLabel"
				android:layout_height="wrap_content"
				style="@style/clendarworkingdaylable"
				android:layout_width="wrap_content" android:text="We" />
			<TextView android:id="@+id/TueLabel"
				android:layout_height="wrap_content"
				style="@style/clendarworkingdaylable"
				android:layout_width="wrap_content" android:text="Th" />
			<TextView android:id="@+id/FriLabel"
				android:layout_height="wrap_content"
				style="@style/clendarworkingdaylable"
				android:layout_width="wrap_content" android:text="Fr" />
			<TextView android:id="@+id/SatLabel"
				android:layout_height="wrap_content"
				style="@style/clendarworkingdaylable"
				android:layout_width="wrap_content" android:text="Sa" />
			<TextView android:id="@+id/SunLabel"
				android:layout_height="wrap_content"
				style="@style/clendarworkingdaylable"
				android:layout_width="wrap_content" android:text="Su" />
		</TableRow>


		<com.welmo.calendar.CalendarWeek android:id="@+id/MonthRow1">
			<com.welmo.calendar.CalendarDay android:layout_column="1" android:id="@+id/Mon1"
				android:text="26" style="@style/clendaroutofmonth" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Twe1"
				android:text="27" style="@style/clendaroutofmonth" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Wen1"
				android:text="28" style="@style/clendaroutofmonth" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Tue1"
				android:text="29" style="@style/clendaroutofmonth" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Fri1"
				android:text="30" style="@style/clendaroutofmonth" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Sat1"
				android:text="31" style="@style/clendaroutofmonth" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Sun1"
				android:text="1" style="@style/clendarfreeday" />
		</com.welmo.calendar.CalendarWeek>

		<com.welmo.calendar.CalendarWeek android:id="@+id/MonthRow2">
			<com.welmo.calendar.CalendarDay android:layout_column="1" android:id="@+id/Mon2"
				android:text="2" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Twe2"
				android:text="3" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Wen2"
				android:text="4" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Tue2"
				android:text="5" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Fri2"
				android:text="6" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Sat2"
				android:text="7" style="@style/clendarfreeday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Sun2"
				android:text="8" style="@style/clendarfreeday" />
		</com.welmo.calendar.CalendarWeek>

		<com.welmo.calendar.CalendarWeek android:id="@+id/MonthRow3">
			<com.welmo.calendar.CalendarDay android:layout_column="1" android:id="@+id/Mon3"
				android:text="9" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Twe3"
				android:text="10" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Wen3"
				android:text="11" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Tue3"
				android:text="12" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Fri3"
				android:text="13" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Sat3"
				android:text="14" style="@style/clendarfreeday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Sun3"
				android:text="15" style="@style/clendarfreeday" />
		</com.welmo.calendar.CalendarWeek>
		<com.welmo.calendar.CalendarWeek android:id="@+id/MonthRow4">
			<com.welmo.calendar.CalendarDay android:layout_column="1" android:id="@+id/Mon4"
				android:text="16" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Twe4"
				android:text="17" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Wen4"
				android:text="18" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Tue4"
				android:text="19" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Fri4"
				android:text="20" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Sat4"
				android:text="21" style="@style/clendarfreeday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Sun4"
				android:text="22" style="@style/clendarfreeday" />
		</com.welmo.calendar.CalendarWeek>

		<com.welmo.calendar.CalendarWeek android:id="@+id/MonthRow5">
			<com.welmo.calendar.CalendarDay android:layout_column="1" android:id="@+id/Mon5"
				android:text="23" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Twe5"
				android:text="24" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Wen5"
				android:text="25" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Tue5"
				android:text="26" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Fri5"
				android:text="27" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Sat5"
				android:text="28" style="@style/clendarfreeday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Sun5"
				android:text="29" style="@style/clendarfreeday" />
		</com.welmo.calendar.CalendarWeek>

		<com.welmo.calendar.CalendarWeek android:id="@+id/MonthRow6">
			<com.welmo.calendar.CalendarDay android:layout_column="1" android:id="@+id/Mon6"
				android:text="30" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Twe6"
				android:text="31" style="@style/clendarworkingday" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Wen6"
				android:text="1" style="@style/clendaroutofmonth" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Tue6"
				android:text="2" style="@style/clendaroutofmonth" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Fri6"
				android:text="3" style="@style/clendaroutofmonth" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Sat6"
				android:text="4" style="@style/clendaroutofmonth" />
			<com.welmo.calendar.CalendarDay android:id="@+id/Sun6"
				android:text="5" style="@style/clendaroutofmonth" />
		</com.welmo.calendar.CalendarWeek>
	</com.welmo.calendar.CalendarMonth>

	<LinearLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="vertical" android:id="@+id/MeetingDayShort"
		android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:layout_weight="1" android:textSize="15px">
		<com.welmo.calendar.CalendarDayMeetingListShort
			android:layout_width="fill_parent"
			android:id="@+id/MeetingDayShortList"
			android:layout_height="fill_parent">
		</com.welmo.calendar.CalendarDayMeetingListShort>
	</LinearLayout>
	<LinearLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="vertical" android:id="@+id/CalendarWeekHour"
		android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:layout_weight="1" android:textSize="15px">
		<ScrollView
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:scrollbars="vertical"
			android:scrollbarSize="5px" android:scrollY="10px">

			<TableLayout android:layout_width="fill_parent"
				android:id="@+id/CalendarWeekHour"
				android:layout_height="wrap_content" android:stretchColumns="*"
				android:shrinkColumns="*" android:clickable="true">
				<TableRow>
					<com.welmo.calendar.CalendarDayHour
						android:id="@+id/Mon1" android:layout_height="fill_parent"/>
					<com.welmo.calendar.CalendarDayHour
						android:id="@+id/Mon1" android:layout_height="fill_parent"/>
					<com.welmo.calendar.CalendarDayHour
						android:id="@+id/Twe1" android:layout_height="fill_parent"
						style="@style/clendaroutofmonth" />
					<com.welmo.calendar.CalendarDayHour
						android:id="@+id/Wen1" android:layout_height="fill_parent"
						style="@style/clendaroutofmonth" />
					<com.welmo.calendar.CalendarDayHour
						android:id="@+id/Tue1" android:layout_height="fill_parent"
						style="@style/clendaroutofmonth" />
					<com.welmo.calendar.CalendarDayHour
						android:id="@+id/Fri1" android:layout_height="fill_parent"
						style="@style/clendaroutofmonth" />
					<com.welmo.calendar.CalendarDayHour
						android:id="@+id/Sat1" android:layout_height="fill_parent"
						style="@style/clendaroutofmonth" />
					<com.welmo.calendar.CalendarDayHour
						android:id="@+id/Sun1" android:layout_height="fill_parent"
						style="@style/clendarfreeday" />
				</TableRow>
			</TableLayout>
		</ScrollView>
	</LinearLayout>
</LinearLayout>